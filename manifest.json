{
  "manifest_version": 3,
  "name": "caption search",
  "version": "0.4",
  "description": "adds a way of navigating a youtube video by searching the subtitle",
  "icons": {
    "48": "./icons/48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
